  <selectionEntries>
    <selectionEntry id="3b494c42-a126-feac-d085-791bcf6e375e" name="Chryssalid" hidden="false" collective="false" import="true" type="upgrade">
      <rules>
        <rule id="Poisonous Claws###DATA###" name="Poisonous Claws" hidden="false">
          <description>Poison enemies wounded with melee attacks.</description>
        </rule>
        <rule id="Implant###DATA###" name="Implant" hidden="false">
          <description>Victims are implanted with a Chryssalid egg if they are killed by a Chryssalid&apos;s attack (not from poison caused by Poisonous Claws).</description>
        </rule>
        <rule id="Leap###DATA###" name="Leap" hidden="false">
          <description>Allows vertical leaps onto elevated surfaces during movement.</description>
        </rule>
      </rules>
      <costs>
        <cost name="pts" typeId="points" value="3000.0"/>
      </costs>
    </selectionEntry>
    <selectionEntry id="4cefc30d-2710-3c94-6c9c-79a00aeb52ff" name="Cyberdisc" hidden="false" collective="false" import="true" type="upgrade">
      <rules>
        <rule id="Death Blossom###DATA###" name="Death Blossom" hidden="false">
          <description>Project an intense energy field from Closed position, damaging all nearby enemies. 3 turn cooldown.</description>
        </rule>
        <rule id="Bombard###DATA###" name="Bombard" hidden="false">
          <description>Throw or launch grenades over exceptionally long distances.</description>
        </rule>
      </rules>
      <costs>
        <cost name="pts" typeId="points" value="4500.0"/>
      </costs>
    </selectionEntry>
    <selectionEntry id="f909f845-6a6c-85d1-2c65-93fe1811d365" name="Drone" hidden="false" collective="false" import="true" type="upgrade">
      <rules>
        <rule id="Repair###DATA###" name="Repair" hidden="false">
          <description>Repairs robotic units.</description>
        </rule>
        <rule id="Overload###DATA###" name="Overload" hidden="false">
          <description>Overload the unit&apos;s own power source, destroying the unit but causing explosive damage similar to a grenade&apos;s.</description>
        </rule>
      </rules>
      <costs>
        <cost name="pts" typeId="points" value="1100.0"/>
      </costs>
    </selectionEntry>
    <selectionEntry id="917df3fe-ddf1-e1ad-9a2c-bdfc57222535" name="Ethereal" hidden="false" collective="false" import="true" type="upgrade">
      <rules>
        <rule id="Mind Control###DATA###" name="Mind Control" hidden="false">
          <description>Very difficult psi technique that, if successful, grants control of the target for 3 turns. Does not work on robotic enemies.</description>
        </rule>
        <rule id="Mindfray###DATA###" name="Mindfray" hidden="false">
          <description>Use psi abilities in combat to unlock further psi training. Causes the target to lose grip on reality, inflicting penalties to Aim, Will and mobility, and doing 5 base damage. Robotic units are immune. Lasts 2 turns. 1 turn cooldown.</description>
        </rule>
        <rule id="Psi Lance###DATA###" name="Psi Lance" hidden="false">
          <description>Project a bolt of pure psi force at an enemy.</description>
        </rule>
        <rule id="Rift###DATA###" name="Rift" hidden="false">
          <description>Devastate an area with a storm of psi energy. The rift does more damage against targets with low Will, and reduced damage against targets with high Will. 4 turn cooldown.</description>
        </rule>
        <rule id="Psi Drain###DATA###" name="Psi Drain" hidden="false">
          <description>Drain health from an ally.</description>
        </rule>
      </rules>
      <costs>
        <cost name="pts" typeId="points" value="9000.0"/>
      </costs>
    </selectionEntry>
    <selectionEntry id="e9da116a-5bad-539c-6678-b25a0abfef94" name="Floater" hidden="false" collective="false" import="true" type="upgrade">
